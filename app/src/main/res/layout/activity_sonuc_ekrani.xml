<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/questionmenu"
    tools:context=".SonucEkrani">

    <Button
        android:id="@+id/button5"
        android:layout_width="210dp"
        android:layout_height="47dp"
        android:layout_marginBottom="60dp"
        android:backgroundTint="#D2D81B"
        android:onClick="anamenuyedon"
        android:text="Ana Menüye Dön"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="130dp"
        android:text="Değişik Sorular"
        android:textColor="#D6D640"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="231dp"
        android:layout_height="38dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="150dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

        <TextView
            android:id="@+id/textskor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="En Yüksek Skor : "
            android:textColor="#D6D640"
            android:textSize="26sp" />

        <TextView
            android:id="@+id/enyuksekskor2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="15"
            android:textColor="#D6D640"
            android:textSize="26sp"
            tools:layout_editor_absoluteX="260dp"
            tools:layout_editor_absoluteY="296dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="234dp"
        android:layout_height="34dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <TextView
            android:id="@+id/yapılantext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text=" Yapılan Skor      : "
            android:textColor="#D6D640"
            android:textSize="26sp"
            tools:layout_editor_absoluteX="126dp"
            tools:layout_editor_absoluteY="342dp" />

        <TextView
            android:id="@+id/yapıilanskor2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="10"
            android:textColor="#D6D640"
            android:textSize="26sp"
            tools:layout_editor_absoluteX="268dp"
            tools:layout_editor_absoluteY="342dp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>